{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "background": {
    "scripts": [
       "background.js"
    ]
  },
  "permissions": [
  	"tabs"
  ],
  "content_scripts": [
  	{
  		"matches": ["https://www.facebook.com/*", "https://www.reddit.com/*", "https://www.youtube.com/*"],
  		"js": ["content.js"]
  	}
  ]
}
