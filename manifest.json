{
  "name": "WebTimer",
  "version": "1.0",
  "description": "Shows how much time a user has spent on a website",
  "manifest_version": 2,
  "background": {
    "scripts": [
    ]
	},
  "permissions": [
  	"input",
  	"http://*/*",
  	"https://*/*",
  	"storage"
  ],
  "options_ui": {
  	"page": "options.html",
  	"open_in_tab": false
  },
  "browser_action": {
  	"default_popup": "popup.html"
  }
}
